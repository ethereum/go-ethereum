	"devDependencies": {
		"@sindresorhus/tsconfig": "~0.7.0",
		"expect-type": "^0.11.0",
		"tsd": "^0.14.0",
		"typescript": "^4.1.3",
		"xo": "^0.36.1"
	},
	"types": "./index.d.ts",
	"typesVersions": {
		">=4.1": {
			"*": [
				"ts41/*"
			]
		}
	},
	"xo": {
		"rules": {
			"@typescript-eslint/ban-types": "off",
			"@typescript-eslint/indent": "off",
			"node/no-unsupported-features/es-builtins": "off"
		}
	}
