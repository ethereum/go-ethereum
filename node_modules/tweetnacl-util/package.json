  ],
  "author": "TweetNaCl-js contributors",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/dchest/tweetnacl-util-js/issues"
  },
  "homepage": "https://github.com/dchest/tweetnacl-util-js",
  "devDependencies": {
    "browserify": "^16.5.0",
    "tape": "^4.13.0",
    "uglify-js": "^3.7.6"
  },
  "browser": {
    "buffer": false
  }
}
